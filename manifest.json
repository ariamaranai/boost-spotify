{
  "manifest_version": 3,
  "name": "spoticount",
  "version": "0",
  "description": "Show playcount spotify.com",
  "icons": {
    "128": "128.png"
  },
  "omnibox": {
    "keyword": "sp"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [{
    "js": ["main.js"],
    "matches": ["https://open.spotify.com/intl*"],
    "world": "MAIN",
    "run_at": "document_start"
  }]
}
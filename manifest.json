{
  "manifest_version": 3,
  "name": "spoticount",
  "version": "0",
  "description": "Show spotify playcount and boosting",
  "icons": {
    "128": "128.png"
  },
  "content_scripts":[{
    "js": ["main.js"],
    "matches": ["https://open.spotify.com/*"],
    "world": "MAIN",
    "run_at": "document_start"
  }]
}